{
  "compilerOptions": {
    "outDir": "./cache",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2019",
    "sourceMap": false,
    "declaration": false,
    "removeComments": true,
    "incremental": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules",
    "dist",
    "cache"
  ]
}
